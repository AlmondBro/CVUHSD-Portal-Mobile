{
  "expo": {
    "name": "CVUHSD Portal Mobile",
    "slug": "CVUHSD-Portal-Mobile",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "scheme": "portal",
    "infoPlist": {
      "LSApplicationQueriesSchemes": ["lyft"]
    },
    "applinks": {
      "apps": [],
      "details": [{
        "appID": "LKWJEF.io.myapp.example",
        "paths": ["/records/*"]
      }]
    },
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "https",
            "host": "*.myapp.io",
            "pathPrefix": "/records"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ]
  }
}